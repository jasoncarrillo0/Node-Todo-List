    <script type="text/javascript">
        $(function() {
            // make sure background is filled correctly on all pages
            if ($(window).height() > $("body").height()) {
                $("body").addClass("background-fill");
            }

            // these two if blocks make sure the dropdown menu is updated
            if ($(".dropdown-menu").children().length === 0) { 
                $("<a class='dropdown-item'>Lists will show here</a>").appendTo(".dropdown-menu");
            }
            if ($(".box.deleted-item-section").children().length === 0) {
                $("<div class='no-lists-banner'>No items to delete.</div>").appendTo(".box.deleted-item-section");
            }

            // change color of bottom page text for /delete and /create
            if (window.location.href.indexOf("delete") > -1) {
                if ($(".no-lists-banner").length) {
                    $(".bottom-text").addClass("add-clarity");
                }

                if ($(".item").length) {
                    $(".item").addClass("revert-content");
                }
            }
            if (window.location.href.indexOf("create") > -1) {
                $(".bottom-text").addClass("add-clarity");
            }
        });
    </script>
</body>
